    },
    "title_cases": {
      "type": "string"
    },
    "link_cases": {
      "type": "string"
    },
    "cases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case.case"
    },
    "review_title": {
      "type": "string"
    },
    "review_link": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review"
    },
    "question": {
      "type": "component",
      "repeatable": false,
      "component": "other-blocks.qusetions-block"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "other-blocks.seo"
    },
    "spheres": {
      "type": "component",
      "repeatable": true,
      "component": "list-sphere.list"
    },
    "list_result": {
      "type": "component",
      "repeatable": true,
